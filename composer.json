{
    "name": "kauanslr/graphthing",
    "description": "Make your life easier when testing your graphql queries and mutations",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Kauan Sousa",
            "email": "kauanslr@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "6.*",
        "laravel/framework": ">=5.5"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "psr-4": {
            "Kauanslr\\GraphThing\\": "src/GraphThing/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
